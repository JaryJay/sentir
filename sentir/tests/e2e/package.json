{
	"name": "@extension/e2e",
	"version": "0.4.3",
	"description": "E2e tests configuration boilerplate",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"e2e": "wdio run ./config/wdio.browser.conf.ts",
		"clean:node_modules": "bunx rimraf node_modules",
		"clean:turbo": "bunx rimraf .turbo",
		"clean": "bun clean:turbo && bun clean:node_modules"
	},
	"devDependencies": {
		"@extension/tsconfig": "workspace:*",
		"@extension/env": "workspace:*",
		"@wdio/cli": "^9.4.5",
		"@wdio/globals": "^9.4.5",
		"@wdio/local-runner": "^9.1.2",
		"@wdio/mocha-framework": "^9.1.2",
		"@wdio/spec-reporter": "^9.2.14",
		"@wdio/types": "^9.1.2"
	}
}
